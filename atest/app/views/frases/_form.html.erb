<%= form_for @frase do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :autor %><br />
    <%= f.text_field :autor %>
  </p>
  <p>
    <%= f.label :frase %><br />
    <%= f.text_area :frase %>
  </p>
  <p>
    <%= f.label :created_by %><br />
    <%= f.text_field :created_by %>
  </p>
  <p><%= f.submit %></p>
<% end %>
