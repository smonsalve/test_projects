<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(comite) -%>">
  <span class='title'>
    <%= comite.nombre %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.comites_comite_path(comite),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_comites_admin_comite_path(comite),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.comites_admin_comite_path(comite),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => comite.nombre),
        :method => :delete %>
  </span>
</li>
