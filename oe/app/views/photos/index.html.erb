<h1>Listing photos</h1>

<%= javascript_tag do%>
window.photosURL = "<%=j photos_url %>";
<% end %>
<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @photos.each do |photo| %>
  <tr>
	  
    <td><%= photo.name %></td>
    <td><%= photo.description %></td>
    <td><%= link_to 'Show', photo %></td>
    <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
    <td><%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<p>
  Download:
  <%= link_to "CSV", photos_path(format: "csv") %> |
  <%= link_to "Excel", photos_path(format: "xls") %>
</p>

<%= link_to 'New Photo', new_photo_path %>

<h2> Import</h2>

<%= form_tag import_photos_path,multipart: true do %>
<%=file_field_tag :file%>
<%= submit_tag "Importar"%>
<% end %>